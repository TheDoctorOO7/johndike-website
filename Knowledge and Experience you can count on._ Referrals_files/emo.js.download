								jQuery(this).parent("li").parent("ul").parent("li").parent("ul").siblings("a").addClass("emoCurrent");
								jQuery(this).parent("li").parent("ul").parent("li").parent("ul").parent("li").addClass("emoCurrent");
							}
						}
					}
				}
			}
		});
	}
	
